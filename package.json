{
    "name": "ul-imports",
    "version": "1.0.0",
    "description": "The scripts used to import data into the Unified Listing.",
    "main": "index.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "node node_modules/nyc/bin/nyc.js node tests/all-tests.js",
        "full-sync": "npm run all-imports && npm run curation-known-duplicates && npm run unifier && npm run sai-curation && npm run curation-unified-orphans && npm run all-image-imports ",
        "all-imports": "npm run eastin-import && npm run sai-import",
        "eastin-import": "node src/js/eastin/launcher.js -- --noCache",
        "sai-import": "node src/js/sai/index.js -- --noCache",
        "gari-import": "node src/js/gari/index.js -- --noCache",
        "sai-sync": "npm run sai-import && npm run unifier && npm run sai-curation",
        "unifier": "node src/js/unifier/index.js -- --optionsFile %ul-imports/configs/unifier-prod.json",
        "all-image-imports": "npm run eastin-image-import && npm run sai-image-import",
        "eastin-image-import": "node src/js/images/eastin-image-sync.js",
        "sai-image-import": "node src/js/images/sai-image-sync.js",
        "gari-image-import": "node src/js/images/gari-image-sync.js",
        "curation-check-source-urls": "node src/js/curation/check-source-urls.js",
        "curation-fix-duplicate-children": "node src/js/curation/fix-duplicate-children.js",
        "curation-fix-duplicate-source-urls": "node src/js/curation/fix-duplicate-source-urls.js",
        "curation-invalid-records": "node src/js/curation/invalid-records.js",
        "curation-known-duplicates": "node src/js/curation/fix-known-duplicates.js",
        "curation-unified-orphans": "node src/js/curation/fix-orphaned-unified-records.js",
        "image-curation": "npm run image-curation-missing-files && npm run image-curation-md5-checksums && npm run image-curation-orphaned-files",
        "image-curation-missing-files": "node src/js/curation/delete-image-records-missing-files.js",
        "image-curation-md5-checksums": "node src/js/curation/add-missing-md5-checksums.js",
        "image-curation-orphaned-files": "node src/js/curation/delete-orphaned-image-files.js",
        "sai-curation": "npm run sai-curation-merge && npm run sai-curation-metadata",
        "sai-curation-delete": "node src/js/sai/delete.js",
        "sai-curation-merge": "node src/js/sai/merge.js",
        "sai-curation-metadata": "node src/js/sai/metadata.js",
        "sai-curation-merge-loops": "node src/js/curation/fix-merge-loops.js",
        "sai-curation-missing-records": "node src/js/curation/flag-missing-sai-records-as-deleted.js",
        "sai-curation-duplicate-children": "node src/js/curation/fix-duplicate-children.js",
        "updates-report": "npm run updates-report:diffs && npm run updates-report:reports",
        "updates-report:diffs": "node src/js/updates-report/process-incoming-import-results.js",
        "updates-report:reports": "node src/js/updates-report/process-incoming-diff-results.js",
        "noop": "echo This container should be launched manually using a command like: \\'docker-compose run ul-imports full-sync\\'."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/the-t-in-rtf/ul-import.git"
    },
    "author": "Tony Atkins <tony@raisingthefloor.org>",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/the-t-in-rtf/ul-import/issues"
    },
    "homepage": "https://github.com/the-t-in-rtf/ul-import#readme",
    "dependencies": {
        "cheerio": "1.0.0-rc.3",
        "fast-csv": "3.4.0",
        "foundation-sites": "6.4.4-rc1",
        "fuse.js": "6.4.1",
        "fluid-diff": "1.0.0",
        "fluid-express": "1.0.16",
        "fluid-launcher": "1.0.0",
        "gpii-sort": "1.0.0-dev.20170420T114825Z.804ac19",
        "gpii-universal": "0.3.0-dev.20191014T122830Z.3b83f5651",
        "infusion": "3.0.0-dev.20191009T141140Z.32c9263b4.FLUID-6148",
        "json5": "2.1.3",
        "juice": "7.0.0",
        "kettle": "1.15.1",
        "markdown-it": "11.0.0",
        "md5-file": "5.0.0",
        "mkdirp": "1.0.4",
        "nodemailer": "6.4.11",
        "nodemailer-smtp-transport": "2.7.4",
        "pako": "1.0.11",
        "request": "2.88.2",
        "rimraf": "3.0.2"
    },
    "devDependencies": {
        "eslint": "7.5.0",
        "eslint-config-fluid": "1.4.0",
        "eslint-plugin-markdown": "1.0.2",
        "fluid-couchdb-test-harness": "1.1.0",
        "fluid-express-user": "1.0.2-dev.20200731T100501Z.31a286a.FLUID-6524",
        "fluid-grunt-eslint": "18.1.2",
        "fluid-grunt-lint-all": "1.0.8",
        "grunt": "1.2.1",
        "node-jqunit": "1.1.8",
        "nyc": "15.1.0",
        "ul-api": "1.0.0-dev.20200731T124130Z.aa35fff.UL-65"
    }
}
